#include "ap.h"
#include "led.h"
#include "cmsis_os.h"

// Global Variables
uint8_t receive_data[8];
uint8_t auto_manual;

// Initialization of app
void apInit() {
	// Start PWM
	HAL_TIM_PWM_Start(&htim1, TIM_CHANNEL_2);
	HAL_TIM_PWM_Start(&htim3, TIM_CHANNEL_2);

	// Initialization of UART
	HAL_UART_Receive_DMA(&huart1, receive_data, sizeof(receive_data));
}


// New Main Functions
void apMain() {
	osKernelStart();
}
